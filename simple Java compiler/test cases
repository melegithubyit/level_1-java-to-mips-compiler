@@test case Arithmetic@@

int x=1;
int y=2;
int z=x+y;
system.out.println(z)

######################################
@@test case conditional@@

int a=1;
if(a<2)
{
system.out.println(a);
}
#####################################


@@test case for loop@@
for(int i=0; i<10; i++)
{
system.out.println(i);
}

######################################3


@@test case while loop@@
int x=1;
while(x<8)
{
system.out.println(x);
x++;
}
######################################3